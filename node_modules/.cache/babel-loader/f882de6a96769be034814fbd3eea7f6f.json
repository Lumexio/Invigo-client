ACA;IACA;;EAHA,CAvDA;EA6DAC;IACA3B;MACA4B;IACA,CAHA;;IAIA3B;MACA2B;IACA;;EANA,CA7DA;;EAsEAC,YAtEA;;EAwEAC;IACAC;MACA;MACAC;MACAA;QACA;UACAC;QACA;MACA,CAJA;IAKA,CATA;;IAUAC;MACA,OACAzB,iBACAP,cADA,IAEA,yBAFA,IAGAO,2DAJA;IAMA,CAjBA;;IAmBA0B;MACA;MACA;MAEA;IACA,CAxBA;;IA0BAC;MACA;MACA;MACA;IACA,CA9BA;;IAgCAC;MACA;MACA;MACAzC;MAEA;MACAuB;QACA;MACA,CAFA;IAGA,CAzCA;;IA2CAmB;MACA;MACA;QACA;QACA;MACA,CAHA;IAIA,CAjDA;;IAmDAC;MACA;MACA;QACA;QACA;MACA,CAHA;IAIA,CAzDA;;IA2DAC;MACA;QACAC;QACA;QACAC;QACA;QACAC;QACAA,+DACA,qDACA,EAFA;QAGA9C;QACAsB;UACA;QACA,CAFA;MAGA,CAbA,MAaA;QACA;MACA;;MACA;IACA;;EA7EA;AAxEA","names":["getCategorias","deleteCategoria","editCategoria","nombre_categoria","data","dialog","dialogDelete","search","cargando","expanded","headers","text","align","sortable","value","categoriaArray","selectrol","itemsrol","editedIndex","editedItem","id","descripcion_categoria","defaultItem","mounted","window","then","response","catch","e","console","computed","formTitle","watch","val","created","methods","onFocus","stext","document","filterOnlyCapsText","editItem","deleteItem","deleteItemConfirm","close","closeDelete","save","Object","send","url"],"sourceRoot":"src/components/table-lists","sources":["tabla-categoria.vue"],"sourcesContent":["<template>\n  <div class=\"tabla\" id=\"app\">\n    <v-row>\n      <v-col cols=\"12\" sm=\"6\" md=\"4\">\n        <v-text-field\n          v-model=\"search\"\n          label=\"Buscar categoria\"\n          class=\"mx-4\"\n          id=\"onsearch\"\n        ></v-text-field>\n      </v-col>\n    </v-row>\n    <v-app id=\"inspire\">\n      <v-progress-linear\n        height=\"6\"\n        indeterminate\n        color=\"cyan\"\n        :active=\"cargando\"\n      ></v-progress-linear>\n      <v-data-table\n        id=\"tabla\"\n        :headers=\"headers\"\n        show-expand\n        :expanded.sync=\"expanded\"\n        :items=\"categoriaArray\"\n        sort-by=\"cantidad_articulo\"\n        class=\"elevation-1\"\n        :search=\"search\"\n        :custom-filter=\"filterOnlyCapsText.toUpperCase\"\n      >\n        <template v-slot:top>\n          <v-toolbar flat>\n            <v-toolbar-title>Tabla categoria</v-toolbar-title>\n            <v-divider class=\"mx-4\" inset vertical></v-divider>\n            <v-spacer></v-spacer>\n            <v-dialog v-model=\"dialog\" max-width=\"500px\">\n              <v-card>\n                <v-card-title>\n                  <h1 class=\"headline\">{{ formTitle }}</h1>\n                </v-card-title>\n\n                <v-card-text>\n                  <v-container>\n                    <v-row>\n                      <v-col>\n                        <v-text-field\n                          v-model=\"editedItem.nombre_categoria\"\n                          label=\"Nombre\"\n                        ></v-text-field>\n                      </v-col>\n                    </v-row>\n                    <v-row\n                      ><v-col>\n                        <v-textarea\n                          v-model=\"editedItem.descripcion_categoria\"\n                          label=\"Descrpción\"\n                          type=\"text\"\n                        ></v-textarea> </v-col\n                    ></v-row>\n                  </v-container>\n                </v-card-text>\n\n                <v-card-actions>\n                  <v-spacer></v-spacer>\n                  <v-btn\n                    color=\"blue darken-1\"\n                    v-on:keyup.enter=\"save\"\n                    text\n                    @click=\"close\"\n                  >\n                    Cancelar\n                  </v-btn>\n                  <v-btn color=\"blue darken-1\" text @click=\"save\">\n                    Guardar\n                  </v-btn>\n                </v-card-actions>\n              </v-card>\n            </v-dialog>\n            <v-dialog v-model=\"dialogDelete\" max-width=\"500px\">\n              <v-card>\n                <v-card-title class=\"headline\"\n                  >¿Estas seguro de querer eliminarlo?</v-card-title\n                >\n                <v-card-actions>\n                  <v-spacer></v-spacer>\n                  <v-btn color=\"blue darken-1\" text @click=\"closeDelete\"\n                    >Cancelar</v-btn\n                  >\n                  <v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\"\n                    >Aceptar</v-btn\n                  >\n                  <v-spacer></v-spacer>\n                </v-card-actions>\n              </v-card>\n            </v-dialog>\n          </v-toolbar>\n        </template>\n        <template v-slot:[`item.actions`]=\"{ item }\">\n          <v-icon small class=\"mr-2\" @click=\"editItem(item)\">\n            mdi-pencil\n          </v-icon>\n          <v-icon small @click=\"deleteItem(item)\"> mdi-delete </v-icon>\n        </template>\n        <template v-slot:no-data>\n          <span>Datos no disponibles.</span>\n        </template>\n        <template v-slot:expanded-item=\"{ headers, item }\">\n          <td :colspan=\"headers.length\">\n            {{ item.descripcion_categoria }}\n          </td>\n        </template>\n      </v-data-table>\n    </v-app>\n  </div>\n</template>\n\n<script>\n  import {\n    getCategorias,\n    deleteCategoria,\n    editCategoria,\n  } from \"@/api/categorias.js\";\n  import { upperConverter } from \"@/special/uppercases-converter.js\";\n  export default {\n    nombre_categoria: \"tabla-categoria\",\n    data: () => ({\n      dialog: false,\n      dialogDelete: false,\n      search: \"\",\n      cargando: true,\n      expanded: [],\n      headers: [\n        {\n          text: \"Categorias\",\n          align: \"start\",\n          sortable: false,\n          value: \"nombre_categoria\",\n        },\n        { text: \"Acciones\", value: \"actions\", sortable: false, align: \"center\" },\n        { text: \"Descripción\", align: \"start\", value: \"data-table-expand\" },\n      ],\n\n      categoriaArray: [],\n      //variable en la que se deposita la posicion en el selector\n      selectrol: null, //Rol\n\n      //Array en el que se deposita de los selectores.\n      itemsrol: [], //Rol\n\n      editedIndex: -1,\n      editedItem: {\n        id: \"\",\n        nombre_categoria: \"\",\n        descripcion_categoria: \"\",\n      },\n      defaultItem: {\n        id: \"\",\n        nombre_categoria: \"\",\n        descripcion_categoria: \"\",\n      },\n    }),\n    mounted() {\n      this.onFocus();\n      window.Echo.channel(\"categorias\").listen(\"categoriaCreated\", (e) => {\n        this.categoriaArray = e.categorias;\n      });\n      getCategorias(this.categoriaArray)\n        .then((response) => {\n          if (response.stats === 200) {\n            this.cargando = false;\n          }\n        })\n        .catch((e) => {\n          console.log(e);\n          this.cargando = true;\n        });\n    },\n\n    computed: {\n      formTitle() {\n        return this.editedIndex === -1 ? \"New Item\" : \"Editar categoria\";\n      },\n    },\n\n    watch: {\n      dialog(val) {\n        val || this.close();\n      },\n      dialogDelete(val) {\n        val || this.closeDelete();\n      },\n    },\n\n    created() {},\n\n    methods: {\n      onFocus() {\n        let stext = document.getElementById(\"onsearch\");\n        stext;\n        stext = addEventListener(\"keydown\", (e) => {\n          if (e.altKey) {\n            document.getElementById(\"onsearch\").focus();\n          }\n        });\n      },\n      filterOnlyCapsText(value, search) {\n        return (\n          value != null &&\n          search != null &&\n          typeof value === \"string\" &&\n          value.toString().toLocaleUpperCase().indexOf(search) !== -1\n        );\n      },\n\n      editItem(item) {\n        this.editedIndex = this.categoriaArray.indexOf(item);\n        this.editedItem = Object.assign({}, item);\n\n        this.dialog = true;\n      },\n\n      deleteItem(item) {\n        this.editedIndex = this.categoriaArray.indexOf(item);\n        this.editedItem = Object.assign({}, item);\n        this.dialogDelete = true;\n      },\n\n      deleteItemConfirm() {\n        this.categoriaArray.splice(this.editedIndex, 1);\n        let id = this.editedItem.id;\n        deleteCategoria(id);\n\n        this.closeDelete();\n        window.Echo.channel(\"categorias\").listen(\"categoriaCreated\", (e) => {\n          this.itemsc = e.categorias;\n        });\n      },\n\n      close() {\n        this.dialog = false;\n        this.$nextTick(() => {\n          this.editedItem = Object.assign({}, this.defaultItem);\n          this.editedIndex = -1;\n        });\n      },\n\n      closeDelete() {\n        this.dialogDelete = false;\n        this.$nextTick(() => {\n          this.editedItem = Object.assign({}, this.defaultItem);\n          this.editedIndex = -1;\n        });\n      },\n\n      save() {\n        if (this.editedIndex > -1) {\n          Object.assign(this.categoriaArray[this.editedIndex], this.editedItem);\n          let send = this.editedItem;\n          send.nombre_categoria = upperConverter(send.nombre_categoria);\n          let url = \"api/categoria/\";\n          url = url + send.id;\n          url = `${url}?${\"nombre_categoria=\" + send.nombre_categoria}&${\n            \"descripcion_categoria=\" + send.descripcion_categoria\n          }`;\n          editCategoria(url);\n          window.Echo.channel(\"categorias\").listen(\"categoriaCreated\", (e) => {\n            this.itemsc = e.categorias;\n          });\n        } else {\n          this.categoriaArray.push(this.editedItem);\n        }\n        this.close();\n      },\n    },\n  };\n</script>\n\n<style scoped>\n  #tabla {\n    width: 60rem;\n  }\n  .tabla {\n    width: 60rem;\n  }\n</style>"]}]}