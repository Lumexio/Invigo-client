MADA;MAEAjB,oBAFA;MAGAkB;IAHA,CAzBA;IA8BAC;MACAF,MADA;MAEAjB,oBAFA;MAGAkB;IAHA;EA9BA,EAFA;;EAsCAE;IACA;IACAC;MACA;IACA,CAFA;IAGAxB,mCACAyB,IADA,CACAC;MACA;QACA;MACA;IACA,CALA,EAMAC,KANA,CAMAC;MACAC;MACA;IACA,CATA;EAUA,CArDA;;EAuDAC;IACAC;MACA;IACA;;EAHA,CAvDA;EA6DAC;IACA3B;MACA4B;IACA,CAHA;;IAIA3B;MACA2B;IACA;;EANA,CA7DA;;EAsEAC,YAtEA;;EAwEAC;IACAC;MACA;MACAC;MACAA;QACA;UACAC;QACA;MACA,CAJA;IAKA,CATA;;IAUAC;MACA,OACAzB,iBACAP,cADA,IAEA,yBAFA,IAGAO,2DAJA;IAMA,CAjBA;;IAmBA0B;MACA;MACA;MAEA;IACA,CAxBA;;IA0BAC;MACA;MACA;MACA;IACA,CA9BA;;IAgCAC;MACA;MACA;MACAzC;MAEA;MACAuB;QACA;MACA,CAFA;IAGA,CAzCA;;IA2CAmB;MACA;MACA;QACA;QACA;MACA,CAHA;IAIA,CAjDA;;IAmDAC;MACA;MACA;QACA;QACA;MACA,CAHA;IAIA,CAzDA;;IA2DAC;MACA;QACAC;QACA;QACAC;QACA;QACAC;QACAA,+DACA,qDACA,EAFA;QAGA9C;QACAsB;UACA;QACA,CAFA;MAGA,CAbA,MAaA;QACA;MACA;;MACA;IACA;;EA7EA;AAxEA","names":["getCategorias","deleteCategoria","editCategoria","nombre_categoria","data","dialog","dialogDelete","search","cargando","expanded","headers","text","align","sortable","value","categoriaArray","selectrol","itemsrol","editedIndex","editedItem","id","descripcion_categoria","defaultItem","mounted","window","then","response","catch","e","console","computed","formTitle","watch","val","created","methods","onFocus","stext","document","filterOnlyCapsText","editItem","deleteItem","deleteItemConfirm","close","closeDelete","save","Object","send","url"],"sourceRoot":"src/components/table-lists","sources":["tabla-categoria.vue"],"sourcesContent":["<template>\n\t<v-card :dark=\"this.$store.getters.hasdarkflag\">\n\t\t<v-toolbar color=\"transparent\" flat>\n\t\t\t<v-toolbar-title>Tabla categoria</v-toolbar-title>\n\t\t\t<v-divider class=\"ml-4\" inset vertical></v-divider>\n\t\t\t<v-spacer></v-spacer>\n\t\t\t<v-text-field\n\t\t\t\tv-model=\"search\"\n\t\t\t\tlabel=\"Buscar categoria\"\n\t\t\t\tclass=\"mt-4\"\n\t\t\t\tid=\"onsearch\"\n\t\t\t></v-text-field>\n\t\t</v-toolbar>\n\n\t\t<v-data-table\n\t\t\t:headers=\"headers\"\n\t\t\tshow-expand\n\t\t\t:expanded.sync=\"expanded\"\n\t\t\t:items=\"categoriaArray\"\n\t\t\tsort-by=\"cantidad_articulo\"\n\t\t\tclass=\"elevation-1\"\n\t\t\t:search=\"search\"\n\t\t\t:custom-filter=\"filterOnlyCapsText.toUpperCase\"\n\t\t>\n\t\t\t<template v-slot:top>\n\t\t\t\t<v-progress-linear\n\t\t\t\t\theight=\"6\"\n\t\t\t\t\tindeterminate\n\t\t\t\t\tcolor=\"cyan\"\n\t\t\t\t\t:active=\"cargando\"\n\t\t\t\t></v-progress-linear>\n\t\t\t\t<v-dialog\n\t\t\t\t\t:dark=\"$store.getters.hasdarkflag\"\n\t\t\t\t\tv-model=\"dialog\"\n\t\t\t\t\tmax-width=\"500px\"\n\t\t\t\t>\n\t\t\t\t\t<v-card>\n\t\t\t\t\t\t<v-card-title>\n\t\t\t\t\t\t\t<h1 class=\"headline\">{{ formTitle }}</h1>\n\t\t\t\t\t\t</v-card-title>\n\n\t\t\t\t\t\t<v-card-text>\n\t\t\t\t\t\t\t<v-container>\n\t\t\t\t\t\t\t\t<v-row>\n\t\t\t\t\t\t\t\t\t<v-col>\n\t\t\t\t\t\t\t\t\t\t<v-text-field\n\t\t\t\t\t\t\t\t\t\t\tv-model=\"editedItem.nombre_categoria\"\n\t\t\t\t\t\t\t\t\t\t\tlabel=\"Nombre\"\n\t\t\t\t\t\t\t\t\t\t></v-text-field>\n\t\t\t\t\t\t\t\t\t</v-col>\n\t\t\t\t\t\t\t\t</v-row>\n\t\t\t\t\t\t\t\t<v-row\n\t\t\t\t\t\t\t\t\t><v-col>\n\t\t\t\t\t\t\t\t\t\t<v-textarea\n\t\t\t\t\t\t\t\t\t\t\tv-model=\"editedItem.descripcion_categoria\"\n\t\t\t\t\t\t\t\t\t\t\tlabel=\"Descrpción\"\n\t\t\t\t\t\t\t\t\t\t\ttype=\"text\"\n\t\t\t\t\t\t\t\t\t\t></v-textarea> </v-col\n\t\t\t\t\t\t\t\t></v-row>\n\t\t\t\t\t\t\t</v-container>\n\t\t\t\t\t\t</v-card-text>\n\n\t\t\t\t\t\t<v-card-actions>\n\t\t\t\t\t\t\t<v-spacer></v-spacer>\n\t\t\t\t\t\t\t<v-btn\n\t\t\t\t\t\t\t\tcolor=\"blue darken-1\"\n\t\t\t\t\t\t\t\tv-on:keyup.enter=\"save\"\n\t\t\t\t\t\t\t\ttext\n\t\t\t\t\t\t\t\t@click=\"close\"\n\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\tCancelar\n\t\t\t\t\t\t\t</v-btn>\n\t\t\t\t\t\t\t<v-btn color=\"blue darken-1\" text @click=\"save\"> Guardar </v-btn>\n\t\t\t\t\t\t</v-card-actions>\n\t\t\t\t\t</v-card>\n\t\t\t\t</v-dialog>\n\t\t\t\t<v-dialog\n\t\t\t\t\t:dark=\"$store.getters.hasdarkflag\"\n\t\t\t\t\tv-model=\"dialogDelete\"\n\t\t\t\t\tmax-width=\"500px\"\n\t\t\t\t>\n\t\t\t\t\t<v-card>\n\t\t\t\t\t\t<v-card-title class=\"headline\"\n\t\t\t\t\t\t\t>¿Estas seguro de querer eliminarlo?</v-card-title\n\t\t\t\t\t\t>\n\t\t\t\t\t\t<v-card-actions>\n\t\t\t\t\t\t\t<v-spacer></v-spacer>\n\t\t\t\t\t\t\t<v-btn color=\"blue darken-1\" text @click=\"closeDelete\"\n\t\t\t\t\t\t\t\t>Cancelar</v-btn\n\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t<v-btn color=\"blue darken-1\" text @click=\"deleteItemConfirm\"\n\t\t\t\t\t\t\t\t>Aceptar</v-btn\n\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t<v-spacer></v-spacer>\n\t\t\t\t\t\t</v-card-actions>\n\t\t\t\t\t</v-card>\n\t\t\t\t</v-dialog>\n\t\t\t</template>\n\t\t\t<template v-slot:[`item.actions`]=\"{ item }\">\n\t\t\t\t<v-icon small class=\"mr-2\" @click=\"editItem(item)\"> mdi-pencil </v-icon>\n\t\t\t\t<v-icon small @click=\"deleteItem(item)\"> mdi-delete </v-icon>\n\t\t\t</template>\n\t\t\t<template v-slot:no-data>\n\t\t\t\t<span>Datos no disponibles.</span>\n\t\t\t</template>\n\t\t\t<template v-slot:expanded-item=\"{ headers, item }\">\n\t\t\t\t<td :colspan=\"headers.length\">\n\t\t\t\t\t{{ item.descripcion_categoria }}\n\t\t\t\t</td>\n\t\t\t</template>\n\t\t</v-data-table>\n\t</v-card>\n</template>\n\n<script>\nimport {\n\tgetCategorias,\n\tdeleteCategoria,\n\teditCategoria,\n} from \"@/api/categorias.js\";\nimport { upperConverter } from \"@/special/uppercases-converter.js\";\nexport default {\n\tnombre_categoria: \"tabla-categoria\",\n\tdata: () => ({\n\t\tdialog: false,\n\t\tdialogDelete: false,\n\t\tsearch: \"\",\n\t\tcargando: true,\n\t\texpanded: [],\n\t\theaders: [\n\t\t\t{\n\t\t\t\ttext: \"Categorias\",\n\t\t\t\talign: \"start\",\n\t\t\t\tsortable: false,\n\t\t\t\tvalue: \"nombre_categoria\",\n\t\t\t},\n\t\t\t{ text: \"Acciones\", value: \"actions\", sortable: false, align: \"center\" },\n\t\t\t{ text: \"Descripción\", align: \"start\", value: \"data-table-expand\" },\n\t\t],\n\n\t\tcategoriaArray: [],\n\t\t//variable en la que se deposita la posicion en el selector\n\t\tselectrol: null, //Rol\n\n\t\t//Array en el que se deposita de los selectores.\n\t\titemsrol: [], //Rol\n\n\t\teditedIndex: -1,\n\t\teditedItem: {\n\t\t\tid: \"\",\n\t\t\tnombre_categoria: \"\",\n\t\t\tdescripcion_categoria: \"\",\n\t\t},\n\t\tdefaultItem: {\n\t\t\tid: \"\",\n\t\t\tnombre_categoria: \"\",\n\t\t\tdescripcion_categoria: \"\",\n\t\t},\n\t}),\n\tmounted() {\n\t\tthis.onFocus();\n\t\twindow.Echo.channel(\"categorias\").listen(\"categoriaCreated\", (e) => {\n\t\t\tthis.categoriaArray = e.categorias;\n\t\t});\n\t\tgetCategorias(this.categoriaArray)\n\t\t\t.then((response) => {\n\t\t\t\tif (response.stats === 200) {\n\t\t\t\t\tthis.cargando = false;\n\t\t\t\t}\n\t\t\t})\n\t\t\t.catch((e) => {\n\t\t\t\tconsole.log(e);\n\t\t\t\tthis.cargando = true;\n\t\t\t});\n\t},\n\n\tcomputed: {\n\t\tformTitle() {\n\t\t\treturn this.editedIndex === -1 ? \"New Item\" : \"Editar categoria\";\n\t\t},\n\t},\n\n\twatch: {\n\t\tdialog(val) {\n\t\t\tval || this.close();\n\t\t},\n\t\tdialogDelete(val) {\n\t\t\tval || this.closeDelete();\n\t\t},\n\t},\n\n\tcreated() {},\n\n\tmethods: {\n\t\tonFocus() {\n\t\t\tlet stext = document.getElementById(\"onsearch\");\n\t\t\tstext;\n\t\t\tstext = addEventListener(\"keydown\", (e) => {\n\t\t\t\tif (e.altKey) {\n\t\t\t\t\tdocument.getElementById(\"onsearch\").focus();\n\t\t\t\t}\n\t\t\t});\n\t\t},\n\t\tfilterOnlyCapsText(value, search) {\n\t\t\treturn (\n\t\t\t\tvalue != null &&\n\t\t\t\tsearch != null &&\n\t\t\t\ttypeof value === \"string\" &&\n\t\t\t\tvalue.toString().toLocaleUpperCase().indexOf(search) !== -1\n\t\t\t);\n\t\t},\n\n\t\teditItem(item) {\n\t\t\tthis.editedIndex = this.categoriaArray.indexOf(item);\n\t\t\tthis.editedItem = Object.assign({}, item);\n\n\t\t\tthis.dialog = true;\n\t\t},\n\n\t\tdeleteItem(item) {\n\t\t\tthis.editedIndex = this.categoriaArray.indexOf(item);\n\t\t\tthis.editedItem = Object.assign({}, item);\n\t\t\tthis.dialogDelete = true;\n\t\t},\n\n\t\tdeleteItemConfirm() {\n\t\t\tthis.categoriaArray.splice(this.editedIndex, 1);\n\t\t\tlet id = this.editedItem.id;\n\t\t\tdeleteCategoria(id);\n\n\t\t\tthis.closeDelete();\n\t\t\twindow.Echo.channel(\"categorias\").listen(\"categoriaCreated\", (e) => {\n\t\t\t\tthis.itemsc = e.categorias;\n\t\t\t});\n\t\t},\n\n\t\tclose() {\n\t\t\tthis.dialog = false;\n\t\t\tthis.$nextTick(() => {\n\t\t\t\tthis.editedItem = Object.assign({}, this.defaultItem);\n\t\t\t\tthis.editedIndex = -1;\n\t\t\t});\n\t\t},\n\n\t\tcloseDelete() {\n\t\t\tthis.dialogDelete = false;\n\t\t\tthis.$nextTick(() => {\n\t\t\t\tthis.editedItem = Object.assign({}, this.defaultItem);\n\t\t\t\tthis.editedIndex = -1;\n\t\t\t});\n\t\t},\n\n\t\tsave() {\n\t\t\tif (this.editedIndex > -1) {\n\t\t\t\tObject.assign(this.categoriaArray[this.editedIndex], this.editedItem);\n\t\t\t\tlet send = this.editedItem;\n\t\t\t\tsend.nombre_categoria = upperConverter(send.nombre_categoria);\n\t\t\t\tlet url = \"api/categoria/\";\n\t\t\t\turl = url + send.id;\n\t\t\t\turl = `${url}?${\"nombre_categoria=\" + send.nombre_categoria}&${\n\t\t\t\t\t\"descripcion_categoria=\" + send.descripcion_categoria\n\t\t\t\t}`;\n\t\t\t\teditCategoria(url);\n\t\t\t\twindow.Echo.channel(\"categorias\").listen(\"categoriaCreated\", (e) => {\n\t\t\t\t\tthis.itemsc = e.categorias;\n\t\t\t\t});\n\t\t\t} else {\n\t\t\t\tthis.categoriaArray.push(this.editedItem);\n\t\t\t}\n\t\t\tthis.close();\n\t\t},\n\t},\n};\n</script>\n\n<style scoped>\n#tabla {\n\twidth: 60rem;\n}\n.tabla {\n\twidth: 60rem;\n}\n</style>"]}]}