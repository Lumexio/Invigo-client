tatus;\n        proveedor.forEach((element) => {\n          let datos = {\n            id: element.id,\n            nombre_proveedor: element.nombre_proveedor,\n            descripcion: element.descripcion,\n          };\n          if (!datos) return;\n          proveedorArray.push(datos);\n        });\n        resolve({\n          stats, proveedorArray\n        });\n      })\n      .catch((error) => { console.log(error); reject(error); });\n  });\n}\nexport function postProveedores(enviar) {\n  axios\n    .post(\"api/proveedor\", enviar)\n    .then((response) => {\n      if (response.statusText === \"Created\") {\n        store.commit(\"setsuccess\", true);\n\n      }\n    })\n    .catch((e) => {\n      console.log(e.message);\n      if (e) {\n        store.commit(\"setdanger\", true);\n      }\n    });\n}\nexport function deleteProveedores(id) {\n  axios.delete(\"api/proveedor/\" + id).catch((error) => console.log(error));\n}\nexport function editProveedores(url) {\n  axios\n    .put(url)\n    .then((response) => {\n      response;\n    })\n    .catch((error) => console.log(error));\n}\n\nexport default { getProveedores, postProveedores, deleteProveedores, editProveedores };"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AACAD,KAAK,CAACE,QAAN,CAAeC,eAAf,GAAiC,IAAjC;AACAH,KAAK,CAACE,QAAN,CAAeE,OAAf,GAAyB,wBAAzB;AAGA,OAAO,SAASC,cAAT,CAAwBC,cAAxB,EAAwC;EAC7C,OAAO,IAAIC,OAAJ,CAAY,CAACC,OAAD,EAAUC,MAAV,KAAqB;IACtCT,KAAK,CACFU,GADH,CACO,eADP,EAEGC,IAFH,CAEQC,QAAQ,IAAI;MAChB,MAAMC,SAAS,GAAGD,QAAQ,CAACE,IAA3B;MACA,MAAMC,KAAK,GAAGH,QAAQ,CAACI,MAAvB;MACAH,SAAS,CAACI,OAAV,CAAmBC,OAAD,IAAa;QAC7B,IAAIC,KAAK,GAAG;UACVC,EAAE,EAAEF,OAAO,CAACE,EADF;UAEVC,gBAAgB,EAAEH,OAAO,CAACG,gBAFhB;UAGVC,WAAW,EAAEJ,OAAO,CAACI;QAHX,CAAZ;QAKA,IAAI,CAACH,KAAL,EAAY;QACZb,cAAc,CAACiB,IAAf,CAAoBJ,KAApB;MACD,CARD;MASAX,OAAO,CAAC;QACNO,KADM;QACCT;MADD,CAAD,CAAP;IAGD,CAjBH,EAkBGkB,KAlBH,CAkBUC,KAAD,IAAW;MAAEC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MAAoBhB,MAAM,CAACgB,KAAD,CAAN;IAAgB,CAlB1D;EA