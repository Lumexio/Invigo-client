function getArticulos(articulosArray) {\n  return new Promise((resolve, reject) => {\n    axios\n      .get(\"api/articulo/list\")\n      .then((response) => {\n        const articulos = response.data;\n        const stats = response.status;\n        articulos.forEach((element) => {\n          let datos = {\n            id: element.id,\n            nombre_articulo: element.nombre_articulo,\n            cantidad_articulo: element.cantidad_articulo,\n            descripcion_articulo: element.descripcion_articulo, //pendiente\n            precio_articulo: element.precio_articulo, //pendiente\n            nombre_categoria: element.nombre_categoria,\n            nombre_tipo: element.nombre_tipo,\n            nombre_marca: element.nombre_marca,\n            nombre_proveedor: element.nombre_proveedor,\n            nombre_status: element.nombre_status,\n            //campos de ubicaciÃ³n\n            nombre_rack: element.nombre_rack,\n            nombre_travesano: element.nombre_travesano,\n            foto_articulo: element.foto_articulo,\n          };\n          if (!datos) return;\n          articulosArray.push(datos);\n        });\n        resolve({\n          stats, articulosArray\n        });\n      })\n      .catch((error) => { console.log(error); reject(error); });\n  });\n}\nexport function postArticulos(enviar) {\n  axios\n    .post(\"api/articulo/create/\", enviar, {\n      headers: {\n        'Content-Type': 'multipart/form-data'\n      }\n    })\n    .then((response) => {\n      if (response.statusText === \"Created\") {\n        store.commit(\"setsuccess\", true);\n\n      }\n    })\n    .catch((e) => {\n      console.log(e.message);\n      if (e) {\n        store.commit(\"setdanger\", true);\n      }\n    });\n}\nexport function deleteArticulos(id) {\n  axios.delete(\"api/articulo/delete/\" + id).then((response) => { response; /*store.commit(\"increment\", 1);*/ }).catch((error) => console.log(error));\n}\nexport function editArticulos(url, data) {\n  axios\n    .put(url, data, {\n      headers: {\n        'Content-Type': 'multipart/form-data'\n      }\n    })\n    .then((response) => {\n      console.log(\"Datos de edit:\", response);\n      if (response.statusText === \"Created\") {\n        store.commit(\"setsuccess\", true);\n      }\n      //store.commit(\"increment\", 1);\n    })\n    .catch((error) => console.log(error));\n}\n\nexport default { getArticulos, postArticulos, deleteArticulos, editArticulos }"],"mappings":"AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,KAAP,MAAkB,SAAlB;AAEAD,KAAK,CAACE,QAAN,CAAeC,eAAf,GAAiC,IAAjC;AACAH,KAAK,CAACE,QAAN,CAAeE,OAAf,GAAyB,YAAYC,MAAM,CAACC,QAAP,CAAgBC,QAA5B,GAAuC,OAAhE;AAGA,OAAO,SAASC,YAAT,CAAsBC,cAAtB,EAAsC;EAC3C,OAAO,IAAIC,OAAJ,CAAY,CAACC,OAAD,EAAUC,MAAV,KAAqB;IACtCZ,KAAK,CACFa,GADH,CACO,mBADP,EAEGC,IAFH,CAESC,QAAD,IAAc;MAClB,MAAMC,SAAS,GAAGD,QAAQ,CAACE,IAA3B;MACA,MAAMC,KAAK,GAAGH,QAAQ,CAACI,MAAvB;MACAH,SAAS,CAACI,OAAV,CAAmBC,OAAD,IAAa;QAC7B,IAAIC,KAAK,GAAG;UACVC,EAAE,EAAEF,OAAO,CAACE,EADF;UAEVC,eAAe,EAAEH,OAAO,CAACG,eAFf;UAGVC,iBAAiB,EAAEJ,OAAO,CAACI,iBAHjB;UAIVC,oBAAoB,EAAEL,OAAO,CAACK,oBAJpB;UAI0C;UACpDC,eAAe,EAAEN,OAAO,CAACM,eALf;UAKgC;UAC1CC,gBAAgB,EAAEP,OAAO,CAACO,gBANhB;UAOVC,WAAW,EAAER,OAAO,CAACQ,WAPX;UAQVC,YAAY,EAAET,OAAO,CAACS,YARZ;UASVC,gBAAgB,EAAEV,OAAO,CAACU,gBAThB;UAUVC,aAAa,EAAEX,OAAO,CAACW,aAVb;UAWV;UACAC,WAAW,EAAEZ,OAAO,CAACY,WAZX;UAaVC,gBAAgB,EAAEb,OAAO,CAACa,gBAbhB;UAcVC,aAAa,EAAEd,OAAO,CAACc;QAdb,CAAZ;QAgBA,IAAI,CAACb,KAAL,EAAY;QACZb,cAAc,CAAC2B,IAAf,CAAoBd,KAApB;MACD,CAnBD;MAoBAX,OAAO,CAAC;QACNO,KADM;QACCT;MADD,CAAD,CAAP;IAGD,CA5BH,EA6BG4B,KA7BH,CA6BUC,KAAD,IAAW;MAAEC,OAAO,CAACC,GAAR,CAAYF,KAAZ;MAAoB1B,MAAM,CAAC0B,KAAD,CAAN;IAAgB,CA7B1D;EA8BD,CA/BM,CAAP;AAgCD;AACD,OAAO,SAASG,aAAT,CAAuBC,MAAvB,EAA+B;EACpC1C,KAAK,CACF2C,IADH,CACQ,sBADR,EACgCD,MADhC,EACwC;IACpCE,OAAO,EAAE;MACP,gBAAgB;IADT;EAD2B,CADxC,EAMG9B,IANH,CAMSC,QAAD,IAAc;IAClB,IAAIA,QAAQ,CAAC8B,UAAT,KAAwB,SAA5B,EAAuC;MACrC5C,KAAK,CAAC6C,MAAN,CAAa,YAAb,EAA2B,IAA3B;IAED;EACF,CAXH,EAYGT,KAZH,CAYUU,CAAD,IAA