lat\n        color=\"transparent\"\n      >\n        <v-btn\n          v-shortkey=\"['esc']\"\n          icon\n          color=\"dark\"\n          @shortkey=\"onClose()\"\n          @click=\"onClose()\"\n        >\n          <v-icon>mdi-close</v-icon>\n        </v-btn>\n        <v-toolbar-title>Crear categorías</v-toolbar-title>\n      </v-toolbar>\n      <v-row\n        ><v-col>\n          <v-text-field\n            v-model=\"nombre_categoria\"\n            :counter=\"10\"\n            label=\"Nombre categoría\"\n            required\n          ></v-text-field>\n        </v-col>\n      </v-row>\n      <v-row>\n        <v-col>\n          <v-textarea\n            v-model=\"descripcion_categoria\"\n            :counter=\"120\"\n            color=\"teal\"\n          >\n            <template v-slot:label>\n              <div>Descripción categoría <small>(opcional)</small></div>\n            </template>\n          </v-textarea>\n        </v-col>\n      </v-row>\n      <v-card-actions>\n        <v-spacer></v-spacer>\n        <v-btn color=\"grey darken-2\" @click=\"clear\" outlined> Limpiar </v-btn>\n        <v-btn\n          color=\"yellow darken-2\"\n          class=\"mr-4\"\n          v-shortkey=\"['enter']\"\n          @shortkey=\"submit\"\n          @click=\"submit\"\n          outlined\n        >\n          Guardar categoria\n        </v-btn>\n      </v-card-actions>\n    </v-card>\n  </v-dialog>\n</template>\n\n<script>\nimport store from \"@/store\";\nimport { postCategorias } from \"@/api/categorias.js\";\nimport { upperConverter } from \"@/special/uppercases-converter.js\";\nexport default {\n  name: \"crearcategoria\",\n  props: {\n    dialogcategoria: { dafault: false },\n  } /*data de llegado de componente padre creacion*/,\n  data: () => ({\n    nombre_categoria: \"\",\n    descripcion_categoria: \"\",\n  }),\n\n  methods: {\n    onClose() {\n      /*Envia parametro de cierre a componente creación*/\n      this.$emit(\"update:dialogcategoria\", false);\n    },\n    submit() {\n      store.commit(\"setsuccess\", null);\n      store.commit(\"setdanger\", null);\n      this.nombre_categoria = upperConverter(this.nombre_categoria);\n      const formdata = new FormData();\n      formdata.append(\"nombre_categoria\", this.nombre_categoria);\n      formdata.append(\"descripcion_categoria\", this.descripcion_categoria);\n      postCategorias(formdata);\n      this.clear();\n    },\n    clear() {\n      (this.nombre_categoria = \"\"), (this.descripcion_categoria = \"\");\n    },\n  },\n};\n</script>\n\n<style scoped>\n.cont-card {\n  padding: 1rem;\n}\n</style>"]}]}